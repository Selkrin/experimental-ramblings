		scrollPane_1.setBounds(536, 290, 248, 274);
		frame.getContentPane().add(scrollPane_1);
		
		textArea_Down = new JEditorPane("text/html", "");
		textArea_Down.setBackground(Color.WHITE);
		textArea_Down.setEditable(false);
		textArea_Down.setFont(new Font("Tahoma", Font.PLAIN, 14));
		scrollPane_1.setViewportView(textArea_Down);
		
		lblWordsDown = new JLabel("Words Down:");
		lblWordsDown.setFont(new Font("Tahoma", Font.BOLD, 14));
		scrollPane_1.setColumnHeaderView(lblWordsDown);

		//TransferFocus from one textField to another, after hitting Enter
		Transfer.transfer();
        
		
		checkButton.addActionListener(new ActionListener() {
            public void actionPerformed(ActionEvent e) {
            	CheckPuzzle.checkpuz();        	
            }
		});
		
		newButton.addActionListener(new ActionListener() {
            public void actionPerformed(ActionEvent e) {
            	Random generator = new Random(); //Random Number Generator to choose a Random Puzzle
                int num = generator.nextInt(1)+1;
                switch (num){
                    case 1: 
                    	Puzzles.blankpuz();
                    	Puzzles.puzzle1();
                    	Puzzles.clues1();
                    	break;
                    case 2: 
                    	Puzzles.blankpuz();
                    	//Puzzles.puzzle2();
                    	//Puzzles.clues2();
                    	break;
                    case 3: 
                    	Puzzles.blankpuz();
                    	//Puzzles.puzzle3();
                    	//Puzzles.clues3();
                    	break;
                    case 4: 
                    	Puzzles.blankpuz();
                    	//Puzzles.puzzle4();
                    	//Puzzles.clues4();
                    	break;
                    default:
                    	break;
                }
            	
            }
		});
		
	}

	private MaskFormatter createFormatter(String s) {
		MaskFormatter formatter = null;
	    try {
	        formatter = new MaskFormatter(s);
	    } catch (java.text.ParseException exc) {
	        System.err.println("formatter is bad: " + exc.getMessage());
	        System.exit(-1);
	    }
	    return formatter;
	}
}
